  "description": "Typescript Memcached Wrapper, with Promise and Template helper",
  "main": "./dst/index.js",
  "typings": "./dst/index.d.ts",
  "scripts": {
    "clean": "rm -Rf dst",
    "prebuild": "npm run clean",
    "build": "tsc -d -p tsconfig.test.json",
