{
  "name": "memcached-typed",
  "version": "5.0.0",
  "description": "Typescript Memcached Wrapper, with Promise and Template helper",
  "main": "./dst/index.js",
  "typings": "./dst/index.d.ts",
  "scripts": {
    "clean": "rm -Rf dst",
    "prebuild": "npm run clean",
    "build": "tsc -d -p tsconfig.test.json",
    "prepublishOnly": "npm run build",
    "pretest": "npm run build",
    "lint": "tslint -c tslint.json src/**/*.ts",
    "test": "MEMCACHED_URL=127.0.0.1:11211 REDIS_URL=redis://127.0.0.1/3 mocha -t 10000 --exit dst/**/__test__/**/*.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/balmbees/memcached-typed.git"
  },
  "author": "Kurt Lee",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/balmbees/memcached-typed/issues"
  },
  "homepage": "https://github.com/balmbees/memcached-typed#readme",
  "devDependencies": {
    "@types/chai": "4.1.3",
    "@types/mocha": "5.2.7",
    "@types/node": "8.10.49",
    "@vingle/tslint-preset": "1.0.0",
    "chai": "4.1.2",
    "mocha": "6.1.4",
    "np": "4.0.2",
    "tslint": "5.18.0",
    "typescript": "3.5.2"
  },
  "dependencies": {
    "@types/ioredis": "^3.2.7",
    "@vingle/memcache-plus": "^0.3.4",
    "ioredis": "^3.2.2"
  }
}
