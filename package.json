{
  "name": "memcached-typed",
  "version": "6.0.1",
  "description": "Typescript Memcached Wrapper, with Promise and Template helper",
  "main": "./dst/index.js",
  "typings": "./dst/index.d.ts",
  "scripts": {
    "clean": "rm -Rf dst",
    "prebuild": "npm run clean",
    "build": "tsc -d -p tsconfig.test.json",
    "prepublishOnly": "npm run build",
    "pretest": "npm run build",
    "lint": "tslint -c tslint.json src/**/*.ts",
    "test": "REDIS_URL=redis://127.0.0.1/3 mocha -t 10000 --exit dst/**/__test__/**/*.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/balmbees/memcached-typed.git"
  },
  "author": "Kurt Lee",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/balmbees/memcached-typed/issues"
  },
  "homepage": "https://github.com/balmbees/memcached-typed#readme",
  "devDependencies": {
    "@commitlint/cli": "8.1.0",
    "@commitlint/config-conventional": "8.1.0",
    "@semantic-release/changelog": "^3.0.4",
    "@semantic-release/git": "^7.0.16",
    "@types/chai": "4.1.7",
    "@types/mocha": "5.2.7",
    "@types/node": "8.10.49",
    "@vingle/tslint-preset": "1.0.0",
    "chai": "4.2.0",
    "cz-conventional-changelog": "^2.1.0",
    "husky": "3.0.0",
    "mocha": "6.1.4",
    "tslint": "5.18.0",
    "typescript": "^3.5.3"
  },
  "dependencies": {
    "@types/ioredis": "^4.0.13",
    "ioredis": "^4.11.2"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  }
}
