{
  "name": "memcached-typed",
  "version": "7.0.0",
  "description": "Typescript cached fetching helper",
  "main": "./dst/index.js",
  "typings": "./dst/index.d.ts",
  "scripts": {
    "clean": "rm -Rf dst",
    "prebuild": "npm run clean",
    "build": "tsc -d -p tsconfig.test.json",
    "prepublishOnly": "npm run build",
    "pretest": "npm run build",
    "lint": "tslint -c tslint.json src/**/*.ts",
    "test": "REDIS_URL=redis://127.0.0.1/3 mocha -t 10000 --exit dst/**/__test__/**/*.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/balmbees/memcached-typed.git"
  },
  "author": "Kurt Lee",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/balmbees/memcached-typed/issues"
  },
  "homepage": "https://github.com/balmbees/memcached-typed#readme",
  "devDependencies": {
    "@types/chai": "4.2.11",
    "@types/mocha": "5.2.7",
    "@types/node": "8.10.59",
    "@vingle/commitlint-preset": "1.0.3",
    "@vingle/semantic-release-config": "1.0.3",
    "@vingle/tslint-preset": "1.0.0",
    "chai": "4.2.0",
    "husky": "3.0.9",
    "mocha": "6.2.2",
    "tslint": "5.20.1",
    "typescript": "3.7.2"
  },
  "dependencies": {
    "@types/ioredis": "^4.0.13",
    "ioredis": "^4.11.2"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "commitlint": {
    "extends": [
      "@vingle/commitlint-preset"
    ]
  }
}
