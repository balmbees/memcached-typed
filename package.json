{
  "name": "@serverless-seoul/cache",
  "version": "7.0.0",
  "description": "Typescript cached fetching helper",
  "main": "./dst/index.js",
  "typings": "./dst/index.d.ts",
  "scripts": {
    "clean": "rm -Rf dst",
    "prebuild": "npm run clean",
    "build": "tsc -d -p tsconfig.test.json",
    "prepublishOnly": "npm run build",
    "pretest": "npm run build",
    "lint": "tslint -c tslint.json src/**/*.ts",
    "test": "mocha -t 10000 --exit dst/**/__test__/**/*.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/balmbees/memcached-typed.git"
  },
  "author": "Kurt Lee",
  "license": "ISC",
  "engines": {
    "node": "^12.10.0 || ^14.0.0"
  },
  "bugs": {
    "url": "https://github.com/balmbees/memcached-typed/issues"
  },
  "homepage": "https://github.com/balmbees/memcached-typed#readme",
  "devDependencies": {
    "@types/chai": "^4.2.14",
    "@types/mocha": "^8.2.0",
    "@types/node": "^12.19.12",
    "@vingle/commitlint-preset": "1.0.3",
    "@vingle/semantic-release-config": "1.0.3",
    "@vingle/tslint-preset": "1.0.0",
    "chai": "^4.2.0",
    "husky": "^4.3.7",
    "mocha": "^8.2.1",
    "tslint": "^6.1.3",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "@types/ioredis": "^4.17.11",
    "ioredis": "^4.19.4"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "commitlint": {
    "extends": [
      "@vingle/commitlint-preset"
    ]
  }
}
