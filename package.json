  "devDependencies": {
    "@types/chai": "^4.1.3",
    "@types/mocha": "^2.2.48",
    "@types/node": "^8.10.49",
    "chai": "^4.1.2",
    "mocha": "^3.5.3",
    "publish": "^0.6.0",
    "typescript": "^3.5.2"
  },
  "dependencies": {
    "@types/ioredis": "^3.2.7",
